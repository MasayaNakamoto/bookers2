<h1>User Info</h1>
<!-- form_with部分 -->
<%= form_with model: @user, url:'/users', local:true do |f| %>
  <h4>名前</h4>
  <%= f.text_field :name %>
  <h4>Image</h4>
  <%= f.attachment_field :profile_image %>
  <h4>Introduction</h4>
  <%= f.text_area :introduction %>
  <%= f.submit 'Update User' %>
<% end %>